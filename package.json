{
  "name": "spore",
  "version": "1.0.0",
  "description": "An application for interacting with environmental control units from Sojourn Fare",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "NODE_ENV=dev concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=true NODE_ENV=production npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SojournFare/spore.git"
  },
  "author": "Justin Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SojournFare/spore/issues"
  },
  "homepage": "https://github.com/SojournFare/spore#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.14",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0",
    "nodemon": "^1.18.7",
    "node-cron": "^2.0.3",
    "node-localstorage": "^1.3.1"
  },
  "devDependencies": {
    "node-dht-sensor": "0.0.34",
    "onoff": "^3.2.2"
  }
}
